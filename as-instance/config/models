import AS.Types

User
    ident Text
    password Text Maybe
    UniqueUser ident
    deriving Typeable
Email
    email Text
    user UserId Maybe
    verkey Text Maybe
    UniqueEmail email

ASFunc
	alias Text
	apply Text
	import Text
	lang Text
	deriving Show

ASCell
    locationString Text
    expressionString Text
    valueString Text

ASDAG
    edge (ASCell, ASCell)
