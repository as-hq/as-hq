import AS.Types

User
    ident Text
    password Text Maybe
    UniqueUser ident
    deriving Typeable
Email
    email Text
    user UserId Maybe
    verkey Text Maybe
    UniqueEmail email

ASFunc
	alias Text
	apply Text
	import Text
	lang Text
	deriving Show

ASCell
    location ASLocation
    expression ASExpression
    value ASValue

ASDAG
    edge (ASCell, ASCell)
